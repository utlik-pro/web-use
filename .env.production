# ========================================
# КОНФИГУРАЦИЯ ДЛЯ DOKPLOY РАЗВЕРТЫВАНИЯ
# ========================================

# Основные LLM API ключи (заполните свои)
OPENAI_ENDPOINT=https://api.openai.com/v1
OPENAI_API_KEY=your_openai_api_key_here

ANTHROPIC_ENDPOINT=https://api.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key_here

GOOGLE_API_KEY=your_google_api_key_here

# Дополнительные LLM провайдеры (опционально)
DEEPSEEK_ENDPOINT=https://api.deepseek.com
DEEPSEEK_API_KEY=your_deepseek_api_key_here

MISTRAL_ENDPOINT=https://api.mistral.ai/v1
MISTRAL_API_KEY=your_mistral_api_key_here

# Настройки приложения
DEFAULT_LLM=openai
ANONYMIZED_TELEMETRY=false
BROWSER_USE_LOGGING_LEVEL=info

# Настройки браузера
BROWSER_DEBUGGING_PORT=9222
BROWSER_DEBUGGING_HOST=localhost
KEEP_BROWSER_OPEN=true
USE_OWN_BROWSER=false

# Настройки дисплея
DISPLAY=:99
RESOLUTION=1920x1080x24
RESOLUTION_WIDTH=1920
RESOLUTION_HEIGHT=1080

# Настройки VNC (для удаленного просмотра браузера)
VNC_PASSWORD=your_secure_vnc_password_here

# Настройки для продакшена
NODE_ENV=production 